{
  "manifestVersion": 1,
  "extensionId": "deploy-ssis",
  "name": "Deploy SSIS",
  "version": "1.0.5",
  "publisher": "mmajcica",
  "description": "Deploy SSIS packages",
  "public": false,
  "galleryFlags": [
    "Public"
  ],
  "icons": {
    "default": "images/logo.png",
    "large": "images/logo-large.png"
  },
  "categories": [
    "Build and release"
  ],
  "tags": [
    "SQL Server Integration Services",
    "SSIS",
    "SQL",
    "ispac"
  ],
  "links": {
    "home": {
      "uri": "http://blog.majcica.com/"
    },
    "support": {
      "uri": "http://blog.majcica.com/2018/04/25/deploy-ssis-packages-from-tfs-vsts-build-release/"
    },
    "repository": {
        "uri": "https://github.com/mmajcica/DeploySsis"
    },
    "issues": {
        "uri": "https://github.com/mmajcica/DeploySsis/issues"
    }
  },
  "repository": {
      "type": "git",
      "uri": "https://github.com/mmajcica/DeploySsis"
  },
  "branding": {
    "color": "#e9ebed",
    "theme": "light"
  },
  "screenshots": [
    {
        "path": "images/ssis-task.png"
    }
  ],
  "content": {
    "details": {
      "path": "README.md",
      "license": { "path": "LICENSE.txt" }
    }
  },
  "files": [
    {
      "path": "images",
      "addressable": true
    },
    {
      "path": "task"
    }
  ],
  "targets": [
    {
      "id": "Microsoft.VisualStudio.Services"
    }
  ],
  "contributions": [
    {
      "id": "DeployIsPacs",
      "type": "ms.vss-distributed-task.task",
      "targets": [
        "ms.vss-distributed-task.tasks"
      ],
      "properties": {
        "name": "task"
      }
    }
  ]
}
